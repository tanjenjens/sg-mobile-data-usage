  pod 'SwiftSpinner'

  target 'Mobile Data UsageTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Mobile Data UsageUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
